{
    "name": "vocaber",
    "version": "1.0",
    "description": "Build an Extension",
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab",
        "http://localhost:5000/"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "options_page": "options.html",
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "commands": {
        "toggle-vober": {
          "suggested_key": {
            "default": "Ctrl+Shift+E",
            "linux": "Ctrl+Shift+E"
          },
          "description": "打开/关闭"
        }
      },
    "manifest_version": 2
}